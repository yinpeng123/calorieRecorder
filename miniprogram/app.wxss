/**app.wxss**/

page {
  background: #faf4de;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.container {
  padding: 0 35rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  line-height: 1.5;
  box-sizing: border-box;
}

.container>view {
  width: 100%;
  box-sizing: border-box;
}

button {
  background: initial;
}

button:focus {
  outline: 0;
}

button::after {
  border: none;
}

.img-icon {
  vertical-align: middle;
}

.text-comment {
  color: #bfbfbf;
  font-size: 0.8em;
}

.text-minor {
  font-size: 0.8em;
  color: #666;
}

.text-prompt {
  font-weight: bold;
  color: #f93;
}

.text-waring {
  color: #f66;
}

.text-title {
  font-weight: bold;
  color: #521a01;
}

.text-low {
  color: #093;
}

.text-bold {
  font-weight: bold;
}

.grey-border-bottom {
  border-bottom: 1px solid #f0f0f0;
}

.margin-bottom-xs {
  margin-bottom: 14rpx;
}

.margin-bottom-sm {
  margin-bottom: 20rpx;
}

.margin-bottom-md {
  margin-bottom: 28rpx;
}

.margin-bottom-lg {
  margin-bottom: 36rpx;
}

.margin-top-xs {
  margin-top: 14rpx;
}

.margin-top-sm {
  margin-top: 20rpx;
}

.margin-top-md {
  margin-top: 28rpx;
}

.margin-top-lg {
  margin-top: 36rpx;
}

.margin-left-xs {
  margin-left: 10rpx;
}

.margin-left-sm {
  margin-left: 15rpx;
}

.margin-left-md {
  margin-left: 20rpx;
}

.margin-left-lg {
  margin-left: 30rpx;
}

.text-fontsize-xs {
  font-size: 28rpx;
}

.text-fontsize-sm {
  font-size: 32rpx;
}

.text-fontsize-md {
  font-size: 36rpx;
}

.text-fontsize-lg {
  font-size: 40rpx;
}

.text-align-center {
  text-align: center;
}

.vertical-align-middle {
  vertical-align: middle;
}

.padding-xs {
  padding: 10rpx;
}

.padding-sm {
  padding: 20rpx;
}

.padding-md {
  padding: 30rpx;
}

.padding-lg {
  padding: 40rpx;
}

@font-face {
  font-family: "iconfont";
  src: url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

